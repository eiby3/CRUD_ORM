@model CRUD_ORM.Controllers.FiboNumero
@{
    ViewData["Title"] = "Privacy Policy";
}
<h1>@ViewData["Title"]</h1>

<p>Use this page to detail your site's privacy policy.</p>


<form method = "post" asp-controller = "Home" asp-action="Fibonacci" autocomplete ="off">
    
        <div class="form-group">
            <p>Digite um número para calcular o Fibonacci</p>
                <label name="Numero" class="control-label"></label>
                <input name="numero" class="form-control" />
            </div>
    <div>
        <input type="submit" value = "Enviar" />
    </div>
    
</form>
<dd class = "col-sm-10">
            @Html.DisplayFor(model => model.resultContent)
</dd>